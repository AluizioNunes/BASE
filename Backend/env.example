# Configurações da Aplicação
APP_NAME=BASE System
DEBUG=False
ENVIRONMENT=production

# Configurações do Banco de Dados
DATABASE_URL=postgresql://BASE:BASE@db:5432/BASE
SQLALCHEMY_LOG_LEVEL=INFO

# Configurações de Autenticação JWT
SECRET_KEY=BASE_SECRET_KEY_CHANGE_IN_PRODUCTION
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Configurações de CORS
BACKEND_CORS_ORIGINS=http://localhost:3000,http://10.10.255.111

# Configurações do Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=BASE

# Configurações do RabbitMQ
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=BASE
RABBITMQ_PASSWORD=BASE

# Configurações do Sentry
SENTRY_DSN=https://exemploPublicKey@o0.ingest.sentry.io/0

# Configurações OAuth - Google
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:8000/api/v1/auth/google/callback

# Configurações OAuth - GitHub
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here
GITHUB_REDIRECT_URI=http://localhost:8000/api/v1/auth/github/callback

# Configurações do Elastic APM
ELASTIC_APM_SERVER_URL=http://localhost:8200
ELASTIC_APM_SECRET_TOKEN=your_apm_secret_token_here

# Configurações de Upload
UPLOAD_DIR=uploads
MAX_FILE_SIZE=10485760
ALLOWED_EXTENSIONS=[".jpg", ".jpeg", ".png", ".gif", ".pdf", ".doc", ".docx", ".xls", ".xlsx"]

# Configurações de Produção
PRODUCTION=True 